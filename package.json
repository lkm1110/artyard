{
  "name": "canvaspop",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --port 8085",
    "start:clear": "expo start --clear --port 8085",
    "android": "expo start --android --port 8085",
    "ios": "expo start --ios --port 8085",
    "web": "expo start --web --port 8085",
    "web:dev": "expo start --web --port 8085 --clear",
    "web:https": "expo start --web --port 8085 --https",
    "build:android": "eas build --platform android --profile production",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios --profile production",
    "build:list": "eas build:list --platform android --limit 5",
    "build:download": "eas build:download",
    "icons:generate": "node scripts/generate-ios-icons.js",
    "icons:install": "npm install --save-dev sharp && npm run icons:generate"
  },
